        "gws_ubiome:sepp_ref_url": "https://storage.gra.cloud.ovh.net/v1/AUTH_a0286631d7b24afba3f3cdebed2992aa/opendata/ubiome/qiime2/sepp-refs-gg-13-8.qza",
        "gws_ubiome:sepp_ref_file": "/data/gws_ubiome/opendata/sepp-refs-gg-13-8.qza",

        "gws_ubiome:greengenes_classifier_url": "https://storage.gra.cloud.ovh.net/v1/AUTH_a0286631d7b24afba3f3cdebed2992aa/opendata/ubiome/qiime2/gg-13-8-99-nb-classifier.qza",
        "gws_ubiome:greengenes_classifier_file": "/data/gws_ubiome/opendata/gg-13-8-99-nb-classifier.qza",

        "gws_ubiome:silva_classifier_url": "https://storage.gra.cloud.ovh.net/v1/AUTH_a0286631d7b24afba3f3cdebed2992aa/opendata/ubiome/qiime2/silva-138-99-nb-classifier.qza",
        "gws_ubiome:silva_classifier_file": "/data/gws_ubiome/opendata/silva-138-99-nb-classifier.qza",

        "gws_ubiome:RDP_classifier_url": "https://storage.gra.cloud.ovh.net/v1/AUTH_a0286631d7b24afba3f3cdebed2992aa/opendata/ubiome/qiime2/RDP_OTUs_classifier.taxa_no_space.v18.202208.qza",
        "gws_ubiome:RDP_classifier_file": "/data/gws_ubiome/opendata/RDP_OTUs_classifier.taxa_no_space.v18.202208.qza",

        "gws_ubiome:ncbi_16s_classifier_url": "https://storage.gra.cloud.ovh.net/v1/AUTH_a0286631d7b24afba3f3cdebed2992aa/opendata/ubiome/qiime2/ncbi-refseqs-classifier.16S_rRNA.20220712.qza",
        "gws_ubiome:ncbi_16s_classifier_file": "/data/gws_ubiome/opendata/ncbi-refseqs-classifier.16S_rRNA.20220712.qza"
    },
    "environment": {
        "pip": [
            {
                "source": "https://pypi.python.org/simple",
                "packages": []
            },
            {
                "source": "https://$GWS_PIP_PACKAGE_TOKEN@gitlab.com/api/.../packages/pypi",
                "packages": []
            }
        ],
        "git": [
            {
                "source": "https://$GWS_GIT_LOGIN:$GWS_GIT_PWD@gitlab.com/gencovery/core",
                "packages":[
                    {"name": "gws_core", "version": "0.3.18", "is_brick": true},
                    {"name": "gws_omix", "version": "0.2.5", "is_brick": true}
                ]
            }
        ],
        "variables": {
            "GWS_GIT_LOGIN": "",
            "GWS_GIT_PWD": "",
            "GWS_PIP_PACKAGE_TOKEN": ""
        }
    }
}